<!DOCTYPE html>

<html lang="en"> 
<head>
  <meta charset="utf-8">
  <title>Widget Notation</title>
  <link rel="stylesheet" href="css/style.css">
</head>

<body>
<script type="text/javascript" src="./js/jquery.min.js"></script>
<script type="text/javascript" src="./js/avis.js"></script>
<script type="text/javascript" src="./js/del.js"></script>



  <div class="note-picker">
    <fieldset class="note-picker-fieldset">
     <input type="radio" name="number" value="1" id="one" checked>
      <label for="one" class="note-picker-label" style="color: #C00">1</label>
      <input type="radio" name="number" value="2" id="two">
      <label for="two" class="note-picker-label" style="color: #F00">2</label>
      <input type="radio" name="number" value="3" id="three">
      <label for="three" class="note-picker-label" style="color: #F60">3</label>
      <input type="radio" name="number" value="4" id="four">
      <label for="four" class="note-picker-label" style="color: #F90">4</label>
      <input type="radio" name="number" value="5" id="five">
      <label for="five" class="note-picker-label" style="color: #FC0">5</label>
      <input type="radio" name="number" value="6" id="six">
      <label for="six" class="note-picker-label" style="color: #FF0">6</label>
      <input type="radio" name="number" value="7" id="seven">
      <label for="seven" class="note-picker-label " style="color: #9F0">7</label>
      <input type="radio" name="number" value="8" id="eight">
      <label for="eight" class="note-picker-label" style="color: #6F0">8</label>
      <input type="radio" name="number" value="9" id="nine">
      <label for="nine" class="note-picker-label" style="color: #3F0">9</label>
      <input type="radio" name="number" value="10" id="ten">
      <label for="ten" class="note-picker-label" style="color: #0F0">10</label>
    </fieldset>    
  </div>

  <section class="about">
    <fieldset class="about-links">
        <label for="pseudo">Pseudo</label></br>
        <input type="text" name="pseudo" id="pseudo">
        <center><p>DÃ©taillez les raisons de votre note</p></center>
        <center><textarea name="positive" id="avis" rows=10 COLS=25 maxlength="200"></textarea></center>
        <center><input type="submit" value="Envoyer" onclick="data()"></center>
    </fieldset>
  </section>
</body>
</html>
